<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.appckathon.appckathon">

    <application
        android:allowBackup="true"
        android:icon="@drawable/appckathon_app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".LoginPage"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterPage"
            android:exported="true" />
        <activity
            android:name=".HomePage"
            android:exported="true"
            android:label="@string/title_activity_home_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CreateHackathon"
            android:exported="true" />
        <activity
            android:name=".UnsignedHackathonPage"
            android:exported="true" />
        <activity
            android:name=".SignedHackathonPage"
            android:exported="true" />
        <activity
            android:name=".ManagedHackathonPage"
            android:exported="true"></activity>
    </application>

</manifest>